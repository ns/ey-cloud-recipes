check process rapns
  with pidfile /data/<%= @app_name %>/shared/pids/rapns.pid
  start program = "cd /data/<%= @app_name %>/current && bundle exec rapns <%= @env %>" as uid <%= @user %> and gid <%= @user %> with timeout 90 seconds
  stop program = "kill -INT `cat /data/<%= @app_name %>/shared/pids/rapns.pid`" as uid <%= @user %> and gid <%= @user %> with timeout 90 seconds